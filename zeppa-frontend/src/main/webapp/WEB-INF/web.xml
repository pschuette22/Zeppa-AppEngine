<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>Upload</servlet-name>
    <servlet-class>com.zeppamobile.frontend.imageupload.Upload
		</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Upload</servlet-name>
    <url-pattern>/image/upload/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetUploadUrl</servlet-name>
    <servlet-class>com.zeppamobile.frontend.imageupload.GetUploadUrl
		</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetUploadUrl</servlet-name>
    <url-pattern>/getuploadurl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>compare-tags</servlet-name>
    <servlet-class>com.zeppamobile.frontend.comparetags.CompareTagsServlet
		</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>compare-tags</servlet-name>
    <url-pattern>/compare-tags</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EmailFormServlet</display-name>
    <servlet-name>EmailFormServlet</servlet-name>
    <servlet-class>com.zeppamobile.frontend.emailform.EmailFormServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmailFormServlet</servlet-name>
    <url-pattern>/email-form</url-pattern>
  </servlet-mapping>
</web-app>