<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>zeppa-cloud-1821</application>
	<version>1</version>
	<threadsafe>true</threadsafe>
	<module>zeppa-smartfollow</module>



	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
	</system-properties>

	<!-- Resource files for libraries -->
	<resource-files>
		<include path="/webapp/WEB-INF/config/**/*" />
		<include path="/webapp/WEB-INF/resources/**/*" />
	</resource-files>

</appengine-web-app>
