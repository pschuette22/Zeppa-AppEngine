<queue-entries>

	<!-- Queue for creating initial follows as people connect -->
	<queue>
		<name>smartfollow-initial</name>
		<mode>pull</mode>
		<retry-parameters>
			<task-retry-limit>5</task-retry-limit>
		</retry-parameters>
	</queue>

	<!-- Queue for creating initial follows as people create new tags -->
	<queue>
		<name>smartfollow-newtag</name>
		<mode>pull</mode>
		<retry-parameters>
			<task-retry-limit>5</task-retry-limit>
		</retry-parameters>
	</queue>

</queue-entries>